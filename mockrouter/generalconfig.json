{
    "server": {
        "port": 8000
    },
    "localoverrides": [],
    "mappings": [
        {
            "paths": [
                "/backend/**"
            ],
            "type": "backend",
            "protocol": "http",
            "host": "localhost",
            "port": 4000
        },
        {
            "paths": [
                "/",
                "/*",
                "/css/**",
                "/fonts/**",
                "/js/**",
                "/src/**",
                "/dist/**",
                "/libs/**",
                "/img/**",
                "/sockjs-node/**"
            ],
            "type": "frontend",
            "protocol": "http",
            "host": "localhost",
            "port": 3000
        }
    ]
}